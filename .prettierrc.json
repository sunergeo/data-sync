{
    "$schema": "https://json.schemastore.org/prettierrc",
    "semi": false,
    "tabWidth": 4,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "none",
    "bracketSpacing": false,
    "arrowParens": "always",
    "proseWrap": "preserve",
    "bracketSameLine": true,
    "vueIndentScriptAndStyle": true,
    "singleAttributePerLine": true
}
